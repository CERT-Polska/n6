[ ca ]
default_ca                      = serviceCA

[ serviceCA ]
dir                             = .
certificate                     = $dir/ca-cert-n6-service-ca.pem
database                        = $dir/index.txt
new_certs_dir                   = $dir/tmp-certs
private_key                     = $dir/ca-key-n6-service-ca.pem
serial                          = $dir/next-serial.txt

default_days                    = 365
default_md                      = sha256
